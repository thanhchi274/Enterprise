    </main>\n    </div>\n  );\n};\n\nexport default PostDetails;\n","/home/user/Enterprise/Enterprise/client/src/components/staff/layout/MainSidebar/SidebarNavItem.js",[],"/home/user/Enterprise/Enterprise/client/src/layouts/user/Default.js",[],"/home/user/Enterprise/Enterprise/client/src/components/user/post/PostListTwo.js",["491","492"],"import React from \"react\";\nimport { Col, Card, CardBody, Badge } from \"shards-react\";\n\nimport PostModal from \"./PostModal\";\n\nconst PostListTwo = ({ post }) => {\n  const [open, setOpen] = React.useState(false);\n\n  const handleOpenModal = (e) => {\n    e.preventDefault();\n    setOpen(true);\n  };\n\n  const handleCloseModal = () => {\n    setOpen(false);\n  };\n\n  return (\n    <Col lg=\"6\" sm=\"12\" className=\"mb-4\">\n      <Card small className=\"card-post card-post--aside card-post--1\">\n        <div\n          className=\"card-post__image\"\n          style={{ backgroundImage: `url('${post.backgroundImage}')` }}\n        >\n          <Badge\n            pill\n            className={`card-post__category bg-${post.categoryTheme}`}\n          >\n            {post.category}\n          </Badge>\n          <div className=\"card-post__author d-flex\">\n            <a\n              href=\"#\"\n              className=\"card-post__author-avatar card-post__author-avatar--small\"\n              style={{ backgroundImage: `url('${post.authorAvatar}')` }}\n            >\n              Written by {post.author}\n            </a>\n          </div>\n        </div>\n        <CardBody>\n          <h5 className=\"card-title\">\n            <a href=\"#\" className=\"text-fiord-blue\" onClick={handleOpenModal}>\n              {post.title}\n            </a>\n          </h5>\n          <p className=\"card-text d-inline-block mb-3\">{post.body}</p>\n          <span className=\"text-muted\">{post.date}</span>\n        </CardBody>\n      </Card>\n      <PostModal post={post} handleCloseModal={handleCloseModal} open={open}/>\n    </Col>\